{
  "name": "@isv-occ-payment/server-extension",
  "version": "1.0.0",
  "private": true,
  "description": "Payment Server Extension",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:tacitknowledge/isv-occ.git"
  },
  "author": "Tacit Knowledge <cybersource-dev@tacitknowledge.com>",
  "main": "cjs/index.js",
  "workspaces": {
    "nohoist": [
      "**/@isv-occ-payment/occ-sdk",
      "**/@isv-occ-payment/payment-sdk",
      "**/cybersource-rest-client",
      "**/form-data",
      "**/jwk-to-pem",
      "**/deepmerge",
      "**/node-cache",
      "**/express-validator",
      "**/guid-typescript",
      "**/jsonwebtoken",
      "**/xml2js",
      "**/@isv-occ-payment/occ-sdk/**",
      "**/@isv-occ-payment/payment-sdk/**",
      "**/cybersource-rest-client/**",
      "**/form-data/**",
      "**/jwk-to-pem/**",
      "**/express-validator/**",
      "**/node-cache/**",
      "**/guid-typescript/**",
      "**/jsonwebtoken/**",
      "**/xml2js/**"
    ]
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc -p tsconfig.json",
    "build:prod": "ttsc -p tsconfig.json",
    "clean": "rimraf ./cjs",
    "prepack": "yarn build",
    "start": "node -r @ts-tools/node/r -r tsconfig-paths/register ./src/server.ts",
    "start:watch": "nodemon",
    "test": "jest",
    "test:coverage:int": "yarn test:int --coverage",
    "test:coverage:unit": "yarn test:unit --coverage",
    "test:int": "yarn test -c jest.int.config.js",
    "test:unit": "yarn test -c jest.unit.config.js"
  },
  "config": {
    "port": "3000",
    "ssl_port": "3001"
  },
  "dependencies": {
    "@isv-occ-payment/occ-sdk": "1.0.0",
    "@isv-occ-payment/payment-gateway": "1.0.0",
    "@isv-occ-payment/payment-sdk": "1.0.0",
    "cybersource-rest-client": "0.0.20",
    "deepmerge": "^4.2.2",
    "express-validator": "6.6.1",
    "form-data": "^3.0.0",
    "guid-typescript": "^1.0.9",
    "jsonwebtoken": "8.5.1",
    "jwk-to-pem": "^2.0.3",
    "node-cache": "5.1.2",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@types/body-parser": "latest",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/nconf": "^0.10.0",
    "@types/superagent-proxy": "^2.0.0",
    "@types/xml2js": "0.4.5",
    "body-parser": "latest",
    "cors": "^2.8.5",
    "express": "latest",
    "nconf": "0.10.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "ttypescript": "^1.5.10",
    "typescript-transform-paths": "^2.0.0",
    "winston": "latest"
  },
  "authenticatedUrls": [],
  "publicUrls": [
    "/ccstorex/custom/isv-payment/v1/paymentMethods",
    "/ccstorex/custom/isv-payment/v1/keys",
    "/ccstorex/custom/isv-payment/v1/payments",
    "/ccstorex/custom/isv-payment/v1/paymentMethods",
    "/ccstorex/custom/isv-payment/v1/applepay/validate",
    "/ccstorex/custom/isv-payment/v1/payerAuth/generateJwt",
    "/ccstorex/custom/isv-payment/v1/report/daily",
    "/ccstorex/custom/isv-payment/v1/report/onDemand",
    "/ccstorex/custom/isv-payment/v1/capture",
    "/ccstorex/custom/isv-payment/v1/refund"
  ],
  "whitelistUrls": [
    "https://apitest.cybersource.com",
    "https://api.cybersource.com"
  ]
}